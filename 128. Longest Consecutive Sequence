


#########version2
class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        nset = set(nums)
        maxl = 0
        for n in nums:
            
            if n-1 not in nset:
                res = 1
                while n+res in nset:
                    res+=1
                maxl=max(maxl, res)
        return maxl
